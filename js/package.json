{
  "name": "codd",
  "version": "0.8.3",
  "description": "JavaScript functional programming library providing relational algebra operations.",
  "homepage": "https://www.fogus.me/",
  "contributors": [
    "Fogus <me@fogus.me> (<http://www.fogus.me>)"
  ],
  "dependencies": {
    "lemonad": ">= 0.7.4"
  },
  "main": "./lib/codd",
  "keywords": [
    "underscore",
    "functional programming",
    "relational algebra"
  ],
  "license": "MIT",
  "engines": {
    "node": "*"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/fogus/codd.git"
  },
  "scripts": {
    "test": "jest",
    "lint": "eslint \"lib/**/*.js\" \"test/**/*.js\"",
    "lint:fix": "eslint --fix \"lib/**/*.js\" \"test/**/*.js\""
  },
  "devDependencies": {
    "@eslint/js": "^9.36.0",
    "eslint": "^9.36.0",
    "jest": "^29.0.0"
  },
  "jest": {
    "testMatch": [
      "**/test/**/*.js"
    ],
    "testPathIgnorePatterns": [
      "<rootDir>/test/lib/"
    ]
  }
